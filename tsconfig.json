{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "./built",
    "allowJs": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "jsx": "react",
    "jsxFactory": "h"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
